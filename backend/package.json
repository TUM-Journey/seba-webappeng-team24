{
  "name": "evaluatione-backend",
  "description": "evalatione-backend seba-24",
  "repository": {
    "type": "git",
    "url": "https://github.com/wingsofovnia/seba-webappeng-team24"
  },
  "author": "SEBA TEAM #24",
  "license": "MIT",
  "private": true,
  "version": "0.0.1",
  "main": "dist",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "brcypt": "^1.0.1",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.15.3",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "mongoose": "^4.10.6",
    "mongoose-unique-validator": "^1.0.5",
    "morgan": "^1.8.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-env": "^1.5.2",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  },
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets env\"",
    "dev-docker": "nodemon -L -w ./src --exec \"babel-node src --presets env\"",
    "build": "babel src -d dist --presets env",
    "run-docker": "node dist/index.js",
    "start": "node dist/index.js",
    "prestart": "npm run -s build",
    "test": "eslint src"
  }
}
