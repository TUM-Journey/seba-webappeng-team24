    "mongoose-unique-validator": "^1.0.5",
    "nconf": "^0.8.4",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
